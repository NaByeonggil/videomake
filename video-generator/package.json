{
  "name": "video-generator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "worker": "tsx src/workers/generateWorker.ts",
    "worker:dev": "tsx watch src/workers/generateWorker.ts",
    "worker:merge": "tsx src/workers/mergeWorker.ts",
    "worker:upscale": "tsx src/workers/upscaleWorker.ts",
    "worker:interpolate": "tsx src/workers/interpolateWorker.ts",
    "worker:export": "tsx src/workers/exportWorker.ts",
    "worker:longvideo": "tsx src/workers/longVideoWorker.ts",
    "workers:all": "concurrently \"npm run worker\" \"npm run worker:merge\" \"npm run worker:upscale\" \"npm run worker:interpolate\" \"npm run worker:export\" \"npm run worker:longvideo\"",
    "test:health": "tsx scripts/checkHealth.ts",
    "test:api": "tsx scripts/testApi.ts",
    "db:reset": "prisma migrate reset --force && prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@tanstack/react-query": "^5.90.20",
    "bullmq": "^5.67.3",
    "ioredis": "^5.9.2",
    "next": "14.2.35",
    "prisma": "^5.22.0",
    "react": "^18",
    "react-dom": "^18",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "uuid": "^13.0.0",
    "ws": "^8.19.0",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/ws": "^8.18.1",
    "concurrently": "^9.2.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.35",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
